{
	"name": "aladinjs",
	"version": "0.1.0",
	"description": "JavaScript library for the Aladin API",
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"files": ["dist"],
	"exports": {
		"require": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.js"
		},
		"import": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.mjs"
		}
	},
	"scripts": {
		"format": "pnpm biome format --write .",
		"build": "tsup"
	},
	"keywords": ["aladin", "api"],
	"engines": {
		"node": ">=18.0.0"
	},
	"author": "Lee Jongwoo <jongwoo328@gmail.com> (https://github.com/jongwoo328)",
	"license": "MIT",
	"devDependencies": {
		"@babel/preset-env": "7.26.9",
		"@babel/preset-typescript": "7.27.0",
		"@biomejs/biome": "1.9.4",
		"@types/jest": "29.5.14",
		"@types/node": "18",
		"jest": "29.7.0",
		"ts-node": "10.9.2",
		"tsup": "^8.4.0",
		"typescript": "^5.8.3"
	}
}
