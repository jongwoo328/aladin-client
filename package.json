{
	"name": "aladin-client",
	"version": "1.0.2",
	"description": "Unofficial JavaScript library for the Aladin API",
	"main": "dist/index.js",
	"type": "module",
	"types": "dist/index.d.ts",
	"sideEffects": false,
	"files": ["dist"],
	"exports": {
		"require": "./dist/index.cjs",
		"import": "./dist/index.mjs"
	},
	"scripts": {
		"format": "pnpm biome format --write .",
		"build": "tsup",
		"test": "jest",
		"prepublishOnly": "pnpm build"
	},
	"packageManager": "pnpm@10.8.1",
	"keywords": [
		"aladin",
		"aladin api",
		"aladin open api",
		"book search",
		"aladin javascript",
		"aladin typescript",
		"aladin unofficial",
		"ttb api"
	],
	"engines": {
		"node": ">=18.0.0"
	},
	"author": "Lee Jongwoo (https://github.com/jongwoo328)",
	"homepage": "https://github.com/jongwoo328/aladin-client#readme",
	"bugs": {
		"url": "https://github.com/jongwoo328/aladin-client/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/jongwoo328/aladin-client"
	},
	"license": "MIT",
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@types/jest": "29.5.14",
		"@types/node": "18",
		"jest": "29.7.0",
		"ts-jest": "29.3.2",
		"tsup": "^8.4.0",
		"typescript": "^5.8.3"
	}
}
